          RowBox[{"s", " ", "\[LessEqual]", " ", "1"}]}], "}"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"r", ",", " ", "s"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"resitev", " ", "\[Equal]", " ", 
        RowBox[{"{", "}"}]}], ",", " ", "\[IndentingNewLine]", 
       RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{"First", " ", "[", 
        RowBox[{
         RowBox[{"AA", " ", "+", " ", 
          RowBox[{"r", " ", 
           RowBox[{"(", 
            RowBox[{"BB", " ", "-", " ", "AA"}], ")"}]}]}], " ", "/.", " ", 
         "resitev"}], " ", "]"}]}], "\[IndentingNewLine]", "]"}]}]}], 
