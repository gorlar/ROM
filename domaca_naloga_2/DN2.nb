   RowBox[{
    RowBox[{"Daljica", "[", 
     RowBox[{"AA_", ",", "BB_"}], "]"}], ",", 
    RowBox[{"Daljica", "[", 
     RowBox[{"CC_", ",", "DD_"}], "]"}]}], "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "resitev", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"resitev", " ", "=", " ", 
      RowBox[{"Solve", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"AA", " ", "+", " ", 
            RowBox[{"r", 
             RowBox[{"(", 
              RowBox[{"BB", " ", "-", " ", "AA"}], ")"}]}]}], " ", "\[Equal]",
